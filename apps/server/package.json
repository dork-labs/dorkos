{
  "name": "@dorkos/server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./services/agent-manager": "./src/services/core/agent-manager.ts",
    "./services/transcript-reader": "./src/services/session/transcript-reader.ts",
    "./services/command-registry": "./src/services/core/command-registry.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:tunnel": "TUNNEL_ENABLED=true TUNNEL_PORT=3000 tsx watch src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "latest",
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "@dorkos/db": "workspace:*",
    "@dorkos/mesh": "workspace:*",
    "@dorkos/relay": "workspace:*",
    "@dorkos/shared": "workspace:*",
    "@ngrok/ngrok": "^1.7.0",
    "@scalar/express-api-reference": "^0.8.40",
    "chokidar": "^4.0.3",
    "conf": "^15.1.0",
    "consola": "^3.4.2",
    "cors": "^2.8.5",
    "croner": "^10.0.1",
    "express": "^4.21.0",
    "gray-matter": "^4.0.3",
    "ulidx": "^2.4.0",
    "uuid": "^10.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@dorkos/test-utils": "workspace:*",
    "@dorkos/typescript-config": "workspace:*",
    "@types/cors": "^2.8.0",
    "@types/express": "^5.0.0",
    "@types/express-serve-static-core": "^5.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "openapi3-ts": "^4.5.0",
    "supertest": "^7.2.2",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
