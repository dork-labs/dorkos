{
  "name": "dorkos",
  "version": "0.3.0",
  "type": "module",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "dotenv -- turbo dev",
    "build": "dotenv -- turbo build",
    "test": "dotenv -- turbo test",
    "typecheck": "dotenv -- turbo typecheck",
    "start": "dotenv -- pnpm --filter=@dorkos/server run start",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docs:export-api": "dotenv -- tsx scripts/export-openapi.ts",
    "test:browser": "dotenv -- turbo run e2e --filter=@dorkos/e2e --only",
    "test:browser:ui": "dotenv -- turbo run e2e:ui --filter=@dorkos/e2e --only",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "drizzle-orm": "^0.39.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^62.5.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "lefthook": "^2.1.1",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "turbo": "^2.8.7",
    "typescript-eslint": "^8.55.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "lefthook",
      "sharp",
      "protobufjs",
      "unrs-resolver",
      "core-js"
    ]
  }
}
